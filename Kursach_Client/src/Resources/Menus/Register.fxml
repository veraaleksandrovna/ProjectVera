<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="428.0" prefWidth="350.0" style="-fx-background-color: #F3D4FF;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Menus.RegisterController">
   <children>
      <PasswordField fx:id="passwordfieldPassword" focusTraversable="false" layoutX="63.0" layoutY="180.0" promptText="Пароль" style="-fx-background-radius: 10;">
         <font>
            <Font size="18.0" />
         </font></PasswordField>
      <PasswordField fx:id="passwordfieldConfirmPassword" focusTraversable="false" layoutX="63.0" layoutY="243.0" promptText="Подтверждение пароля" style="-fx-background-radius: 10;">
         <font>
            <Font size="18.0" />
         </font></PasswordField>
      <Button fx:id="buttonSignUp" focusTraversable="false" layoutX="102.0" layoutY="325.0" mnemonicParsing="false" onAction="#Signup_Pressed" prefHeight="34.0" prefWidth="147.0" style="-fx-background-radius: 10;" text="Регистрация">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font></Button>
      <Button fx:id="buttonBack" focusTraversable="false" layoutX="139.0" layoutY="359.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="34.0" style="-fx-background-radius: 10; -fx-background-color: transparent;" text="Назад" underline="true">
         <font>
            <Font name="Cambria" size="18.0" />
         </font></Button>
      <TextField fx:id="textfieldLogin" focusTraversable="false" layoutX="63.0" layoutY="120.0" promptText="Логин" style="-fx-background-radius: 10;">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Label fx:id="labelMessage" alignment="CENTER" contentDisplay="CENTER" layoutX="65.0" layoutY="298.0" text="error" textFill="RED" visible="false">
         <font>
            <Font name="Cambria" size="16.0" />
         </font></Label>
      <Label layoutX="91.0" layoutY="30.0" text="Регистрация" AnchorPane.leftAnchor="81.0" AnchorPane.rightAnchor="81.0">
         <font>
            <Font name="Cambria Bold" size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
